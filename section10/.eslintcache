;\n    };\n  }, []);\n\n  const { isValid: emailIsValid } = emailState;\n  const { isValid: passwordIsValid } = passwordState;\n\n  useEffect(() => {\n    const identifier = setTimeout(() => {\n      console.log(\"Checking form validity!\");\n      setFormIsValid(emailIsValid && passwordIsValid);\n    }, 500);\n\n    return () => {\n      console.log(\"CLEANUP\");\n      clearTimeout(identifier);\n    };\n  }, [emailState, passwordState]);\n\n  const emailChangeHandler = (event) => {\n    dispatchEmail({ type: \"USER_INPUT\", val: event.target.value });\n\n    setFormIsValid(event.target.value.includes(\"@\") && passwordState.isValid);\n  };\n\n  const passwordChangeHandler = (event) => {\n    dispatchPassword({ type: \"USER_INPUT\", val: event.target.value });\n\n    setFormIsValid(\n      emailState.value.includes(\"@\") && event.target.value.trim().length > 6\n    );\n  };\n\n  const validateEmailHandler = () => {\n    // setEmailIsValid(emailState.isValid);\n    dispatchEmail({ type: \"INPUT_BLUR\" });\n  };\n\n  const validatePasswordHandler = () => {\n    // setPasswordIsValid(enteredPassword.trim().length > 6);\n    dispatchPassword({ type: \"INPUT_BLUR\" });\n  };\n\n  const submitHandler = (event) => {\n    event.preventDefault();\n    props.onLogin(emailState.value, passwordState.value);\n  };\n\n  return (\n    <Card className={classes.login}>\n      <form onSubmit={submitHandler}>\n        <div\n          className={`${classes.control} ${\n            emailState.isValid === false ? classes.invalid : \"\"\n          }`}\n        >\n          <label htmlFor=\"email\">E-Mail</label>\n          <input\n            type=\"email\"\n            id=\"email\"\n            value={emailState.value}\n            onChange={emailChangeHandler}\n            onBlur={validateEmailHandler}\n          />\n        </div>\n        <div\n          className={`${classes.control} ${\n            passwordState.isValid === false ? classes.invalid : \"\"\n          }`}\n        >\n          <label htmlFor=\"password\">Password</label>\n          <input\n            type=\"password\"\n            id=\"password\"\n            value={passwordState.value}\n            onChange={passwordChangeHandler}\n            onBlur={validatePasswordHandler}\n          />\n        </div>\n        <div className={classes.actions}>\n          <Button type=\"submit\" className={classes.btn} disabled={!formIsValid}>\n            Login\n          </Button>\n        </div>\n      </form>\n    </Card>\n  );\n};\n\nexport default Login;\n","C:\\Users\\Messagio\\Desktop\\ReactJS\\ReactPractise\\section10\\src\\components\\MainHeader\\Navigation.js",[],"C:\\Users\\Messagio\\Desktop\\ReactJS\\ReactPractise\\section10\\src\\components\\UI\\Card\\Card.js",[],"C:\\Users\\Messagio\\Desktop\\ReactJS\\ReactPractise\\section10\\src\\components\\UI\\Button\\Button.js",[],"C:\\Users\\Messagio\\Desktop\\ReactJS\\ReactPractise\\section10\\src\\state\\auth-context.js",[],{"ruleId":"77","severity":1,"message":"78","line":65,"column":6,"nodeType":"79","endLine":65,"endColumn":33,"suggestions":"80"},{"ruleId":"81","replacedBy":"82"},{"ruleId":"83","replacedBy":"84"},{"ruleId":"77","severity":1,"message":"78","line":65,"column":6,"nodeType":"79","endLine":65,"endColumn":33,"suggestions":"85"},"react-hooks/exhaustive-deps","React Hook useEffect has missing dependencies: 'emailIsValid' and 'passwordIsValid'. Either include them or remove the dependency array.","ArrayExpression",["86"],"no-native-reassign",["87"],"no-negated-in-lhs",["88"],["89"],{"desc":"90","fix":"91"},"no-global-assign","no-unsafe-negation",{"desc":"90","fix":"92"},"Update the dependencies array to be: [emailIsValid, emailState, passwordIsValid, passwordState]",{"range":"93","text":"94"},{"range":"95","text":"94"},[1872,1899],"[emailIsValid, emailState, passwordIsValid, passwordState]",[1872,1899]]